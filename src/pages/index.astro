---
import Layout from "../layouts/Layout.astro";
import { Image } from 'astro:assets';
import ConeImage from "assets/img/home-3/cone.webp";
import DonutImage from "assets/img/home-3/donut.webp";
import GridImage from "assets/img/home-3/ilust-grids.webp";
---

<Layout title="Jay - Software Agency and creative IT solutions">
    <!-- Start Banner section -->
    <div class="banner-area3">
      <div class="scroll-down">
        <a href="#home3-partner-area">
          Scroll Down
          <span>
            <svg
              width="13"
              height="13"
              viewBox="0 0 13 13"
              fill="none"
              xmlns="http://www.w3.org/2000/svg"
            >
              <path d="M0 1H12M12 1V13M12 1L0.5 12"></path>
            </svg>
          </span>
        </a>
      </div>
      <div class="banner-vector">
        <img
          class="img-fluid"
          src="assets/img/home-3/banner-vector.png"
          alt=""
        />
      </div>
      <div class="swiper-pagination1"></div>
      <div class="social-area">
        <ul>
          <li>
            <a href="https://www.facebook.com/jaysoftwarero/" aria-label="Facebook"
              ><i class="bx bxl-facebook"></i></a>
          </li>
          <li>
            <a href="https://ro.linkedin.com/company/jaysoftware" aria-label="LinkedIn"
              ><i class="bx bxl-linkedin"></i></a
            >
          </li>
          <li>
            <a href="https://www.instagram.com/jay_software/" aria-label="Instagram"
              ><i class="bx bxl-instagram"></i></a>
          </li>
        </ul>
      </div>
      <div class="banner3-slider">
        <div class="banner-wrapper">
          <div class="banner-content">
            <h1>Turn your idea into a market ready product</h1>
            <p>
              Transform your startup’s vision into reality with our custom
              software solutions. Elevate your brand and engage your audience
              through tailored development.
            </p>
            <div class="banner-btn-group">
              <a class="primary-btn3 inline-flex" href="#contact"
                >Let's Talk
                <svg
                  width="27"
                  height="27"
                  viewBox="0 0 27 27"
                  fill="none"
                  xmlns="http://www.w3.org/2000/svg"
                >
                  <path
                    d="M8.08285 6.65259H19.9995V18.5693H17.8328V10.3513L8.08285 20.1013L6.55078 18.5693L16.3008 8.81925H8.08285V6.65259Z"
                    fill="currentColor"></path>
                </svg>
              </a>
            </div>
          </div>
          <div class="magnetic-item">
            <Image class="img-fluid  hidden lg:block" src={ConeImage} height={500} width={500} alt="" loading="eager" />
          </div>
          <div class="magnetic-item">
            <Image class="img-fluid  hidden lg:block" src={DonutImage} height={500} width={500} alt="" loading="eager" />
          </div>
        </div>
      </div>
    </div>
    <!-- End Banner section -->
    <!-- Start Partner section -->
    <div class="home3-partner-area" id="home3-partner-area">
      <div class="container-fluid">
        <div class="row g-4">
          <div class="col-lg-3">
            <div class="partner-title">
              <h3>Our Collaborators</h3>
            </div>
          </div>
          <div class="col-lg-9">
            <div class="partner-wrapper">
              <div class="marquee_text">
                <img
                  src="assets/img/partners/cirkus-logo.svg"
                  style="height: 96px; width: 200px"
                  alt=""
                />
                <img src="assets/img/home-3/star.svg" alt="" />
                <img
                  src="assets/img/partners/bmw-group.svg"
                  style="height: 96px; width: 200px"
                  alt=""
                />
                <img src="assets/img/home-3/star.svg" alt="" />
                <img
                  src="assets/img/partners/logo-pluria.svg"
                  style="height: 96px; width: 200px"
                  alt=""
                />
                <img src="assets/img/home-3/star.svg" alt="" />
                <img
                  src="assets/img/partners/logo_webchain_transparent_alb.png"
                  style="height: 96px; width: 200px"
                  alt=""
                />
                <img src="assets/img/home-3/star.svg" alt="" />
                <img
                  src="assets/img/partners/bright-se-logo.svg"
                  style="height: 96px; width: 200px"
                  alt=""
                />
                <img src="assets/img/home-3/star.svg" alt="" />
                <img
                  src="assets/img/partners/go-1-logo.svg"
                  style="height: 96px; width: 200px"
                  alt=""
                />
                <img src="assets/img/home-3/star.svg" alt="" />
                <img
                  src="assets/img/partners/jba-group-logo.svg"
                  style="height: 96px; width: 200px"
                  alt=""
                />
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- End Partner section -->
    <!-- Start Solution Section -->
    <div class="home3-solution-section sec-mar" id="services">
      <div class="container">
        <div
          class="row mb-55 wow animate fadeInUp"
          data-wow-delay="200ms"
          data-wow-duration="1500ms"
        >
          <div
            class="col-lg-12 d-flex align-items-center justify-content-between gap-4 flex-wrap"
          >
            <div class="section-title-3">
              <h2 aria-label="Our Software Development Services">Our Software Development Services</h2>
              <p>
                Below, you'll discover a comprehensive list of solutions we
                offer as your trusted development partner.
              </p>
            </div>
            <div class="swiper-btn-group">
              <div class="swiper-btn prevbtn1">
                <i class="bi bi-arrow-left"></i>
              </div>
              <div class="swiper-btn nextbtn1">
                <i class="bi bi-arrow-right"></i>
              </div>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-12">
            <div class="swiper home3-solution-slider">
              <div class="swiper-wrapper">
                <div class="swiper-slide">
                  <div class="solution-card">
                    <div class="icon">
